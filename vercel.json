{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/boutique",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "**/*.html": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}